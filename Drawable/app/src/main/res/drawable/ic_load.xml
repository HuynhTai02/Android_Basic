<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:thickness="15dp"
            android:useLevel="false">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item
        android:width="100dp"
        android:height="5dp"
        android:left="50dp"
        android:right="50dp">
        <rotate android:fromDegrees="360">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
                <corners
                    android:bottomLeftRadius="20dp"
                    android:topRightRadius="20dp" />
            </shape>
        </rotate>
    </item>
    <item
        android:width="100dp"
        android:height="5dp"
        android:left="50dp"
        android:right="50dp"
        android:top="50dp">
        <rotate android:fromDegrees="360">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
                <corners
                    android:bottomLeftRadius="20dp"
                    android:topRightRadius="20dp" />
            </shape>
        </rotate>
    </item>
    <item
        android:width="100dp"
        android:height="5dp"
        android:gravity="center"
        android:left="50dp"
        android:right="50dp">
        <rotate android:fromDegrees="360">
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
                <corners
                    android:bottomLeftRadius="20dp"
                    android:topRightRadius="20dp" />
            </shape>
        </rotate>
    </item>

</layer-list>